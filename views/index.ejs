<h1>Stock Dashboard</h1>
<table>
    <tr>
        <th>
            Symbol
        </th>
        <th>
            Description
        </th>
        <th>
            lastPrice
        </th>
        <th>
            Net Change
        </th>
    </tr>
    <% for(let i = 0; i < stocks.length; i++)  {%> 
        <tr>
            <td>
                <a href="/stocks/<%=stocks[i]._id%>"> 
                    <%= stocks[i].symbol %> 
                </a>
            </td>
            <td>
                <%= stocks[i].description %> 
            </td>
            <td>
                <%= stocks[i].lastPrice %> 
            </td>
            <td>
                <%= stocks[i].netChange %> 
            </td>
            <td>
                <form>
                    <button>Delete</button>
                </form>
            </td>
        </tr>
    <% } %> 
    </table>
